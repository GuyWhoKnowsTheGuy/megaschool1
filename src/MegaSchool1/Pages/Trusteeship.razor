@page "/trusteeship"
@inherits PageBase
@using MegaSchool1.Components
@using MegaSchool1.Model

<MudStack Spacing="3">
    <Collapsable Title="Trusteeship">
        <MudStack Spacing="3">
            <Fundable360CapturePageShareable/>
            <ShareableVideo Content="Content.Fundable360" />
            <ShareableVideo Content="Content.Fundable360Training" />
            <MudCard Outlined="true">
                <MudCardHeader>Fundable360 Consultation Prerequisites</MudCardHeader>
                <MudCardContent>
                    <MudText Align="Align.Left">1) You must have an EIN.  Obtain for free through MWR Tax Hotline.</MudText>
                    <MudText Align="Align.Left">2) Have a Business Bank account.</MudText>
                    <MudText Align="Align.Left">3) $2,995 (great to have but not needed at this time)</MudText>
                    <MudText Align="Align.Left">4) Business web page other than MWR</MudText>
                    <MudText Align="Align.Left">5) Business Email</MudText>
                    <MudText Align="Align.Left">6) Business phone number</MudText>
                </MudCardContent>
            </MudCard>
        </MudStack>
    </Collapsable>
</MudStack>